@using Byte.Blog.Framework.Web
<!DOCTYPE html>
<html>
    <head>
        <title>Ben Lakey</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/rendering/assets/styles/bootstrap.min.css")"></link>
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/rendering/assets/styles/main.css")"></link>
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/rendering/assets/styles/markdown.css")"></link>
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/rendering/assets/styles/prettify.css")"></link>
    </head>
    <body>
        <div class="container">
            <div class="row">
                <div class="span12 header">
                    <h1><a href="/">benlakey.com</a></h1>
                </div>
            </div>
            <div class="row">
                <div class="span12">
                    <div id="navigation">
                        @Html.Action("menu", "navigation")
                        <ul class="nav pull-right">
                            <li>
                                <div class="social addthis_toolbox addthis_32x32_style addthis_default_style">
                                    <a class="addthis_button_facebook_follow" addthis:userid="benjaminlakey"></a>
                                    <a class="addthis_button_twitter_follow" addthis:userid="benlakey"></a>
                                    <a class="addthis_button_rss_follow" addthis:url="http://benlakey.com/rss"></a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="span12" id="content-container">
                    <div class="row">
                        <div class="span8">
                            <div class="pad20">
                                @RenderBody()
                            </div>
                        </div>
                        <div class="span4">
                            <div class="sidebar">
                                <div class="well well-small">
                                    
                                    <p>Ben Lakey is a software developer for msnbc.com, an avid gamer, and Seattle resident. The views expressed here are my own and do not necessarily reflect those of my employer.</p>

                                    <h3>Search</h3>
                                
                                    <h3>Stackoverflow</h3>
                                    <!--<a href="http://stackoverflow.com/users/112765/byte">
                                            <img src="http://stackoverflow.com/users/flair/112765.png" border="0" width="208" height="58" alt="profile for byte at Stack Overflow, Q&amp;A for professional and enthusiast programmers" title="profile for byte at Stack Overflow, Q&amp;A for professional and enthusiast programmers" />
                                        </a>-->
                                    <h3>Top Posts</h3>
                                    <ol>
                                        <li>Alpha</li>
                                        <li>Beta</li>
                                        <li>Charlie</li>
                                        <li>Delta</li>
                                    </ol>
                                    <h3>Tags</h3>
                                    <p>.NET ASP.NET MVC C# caching design patterns encapsulationgame development HTML5 Java Javascript LINQ NuGet performance ravendb software craftsmanship software engineering</p>
                                    <h3>Archives</h3>
                                    <ul>
                                        <li>March 2012</li>
                                        <li>February 2012</li>
                                        <li>November 2011</li>
                                        <li>October 2011</li>
                                        <li>September 2011</li>
                                        <li>August 2011</li>
                                        <li>July 2011</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="span12">
                    <p align="center">(c) 2006-2012 Ben Lakey.</p>
                </div>
            </div>
        </div>
        <div id="footer-scripts">
            <script src="@Url.Content("~/rendering/assets/scripts/lib/jquery-1.7.2.min.js")" type="text/javascript"></script>
            <script src="@Url.Content("~/rendering/assets/scripts/lib/underscore-min.js")" type="text/javascript"></script>
            <script src="@Url.Content("~/rendering/assets/scripts/lib/backbone-min.js")" type="text/javascript"></script>
            <script src="@Url.Content("~/rendering/assets/scripts/lib/bootstrap.min.js")" type="text/javascript"></script>
            <script src="@Url.Content("~/rendering/assets/scripts/lib/prettify.js")" type="text/javascript"></script>
            <script src="http://s7.addthis.com/js/250/addthis_widget.js#pubid=xa-4f26479f35b744ef" type="text/javascript"></script>
            
            <script type="text/javascript">
                window.disqus_developer = @(MvcApplication.EnvironmentType == EnvironmentType.Development ? '1' : '0');

                $(document).ready(function () {
                    $('pre').addClass('prettyprint linenums');
                    window.prettyPrint();
                });

            </script>
            
            @RenderSection("FooterScripts", false)


        </div>
    </body>
</html>
